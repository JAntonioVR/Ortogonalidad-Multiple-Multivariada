Cell["Implementaci\[OAcute]n de la CPF", "Subsection",
 CellChangeTimes->{{3.9173291557076845`*^9, 
  3.917329162451847*^9}},ExpressionUUID->"8254625e-f10c-4651-a861-\
01850caff9e6"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"CANTOR", " ", "PAIRING", " ", "FUNCTION"}], ",", " ", 
    RowBox[{
     RowBox[{"implementada", " ", "de", " ", "dos", " ", "maneras", " ", 
      RowBox[{"distintas", ":", "\[IndentingNewLine]", "\t", 
       RowBox[{
        RowBox[{
         RowBox[{"-", " ", "Una"}], " ", "acepta", " ", "como", " ", 
         "argumento", " ", "una", " ", "pareja", " ", "de", " ", 
         "n\[UAcute]meros", " ", "naturales"}], "\[IndentingNewLine]", "   ", 
        "-", " ", 
        RowBox[{
        "La", " ", "otra", " ", "acepta", " ", "dos", " ", "n\[UAcute]meros", 
         " ", "naturales", " ", "como", " ", 
         RowBox[{"argumentos", ".", "\[IndentingNewLine]", "Devuelve"}], " ", 
         "un", " ", "n\[UAcute]mero", " ", "natural", " ", "z", " ", "tal", 
         " ", "que", " ", "z"}]}]}]}], " ", "=", " ", 
     RowBox[{"\[Pi]", 
      RowBox[{"(", 
       RowBox[{"n", ",", "m"}], ")"}]}]}]}], "\[IndentingNewLine]", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"CPF", "[", 
      RowBox[{"{", 
       RowBox[{"n_", ",", "m_"}], "}"}], "]"}], ":=", " ", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "/", "2"}], ")"}], 
       RowBox[{"(", 
        RowBox[{"n", "+", "m"}], ")"}], 
       RowBox[{"(", 
        RowBox[{"n", "+", "m", "+", "1"}], ")"}]}], "+", "m"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"CPF", "[", 
      RowBox[{"n_", ",", "m_"}], "]"}], ":=", 
     RowBox[{"CPF", "[", 
      RowBox[{"{", 
       RowBox[{"n", ",", "m"}], "}"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{
     "INVERSE", " ", "OF", " ", "CANTOR", " ", "PAIRING", " ", "FUNCTION"}], 
     ",", " ", 
     RowBox[{
      RowBox[{
      "acepta", " ", "un", " ", "n\[UAcute]mero", " ", "natural", " ", "z", 
       " ", "como", " ", "argumento", "\[IndentingNewLine]", "Devuelve", " ", 
       "una", " ", "pareja", " ", 
       RowBox[{"{", 
        RowBox[{"x", ",", "y"}], "}"}], " ", "tal", " ", "que", " ", 
       RowBox[{"(", 
        RowBox[{"x", ",", "y"}], ")"}]}], " ", "=", " ", 
      RowBox[{
       SuperscriptBox["\[Pi]", 
        RowBox[{"-", "1"}]], 
       RowBox[{"(", "z", ")"}]}]}]}], "\[IndentingNewLine]", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"CPFinv", "[", "n_", "]"}], ":=", 
    RowBox[{"Block", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"w", ",", "x", ",", "y", ",", "t"}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"w", " ", "=", " ", 
        RowBox[{"Floor", "[", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Sqrt", "[", 
             RowBox[{
              RowBox[{"8", "*", "n"}], "+", "1"}], "]"}], "-", "1"}], ")"}], 
          "/", "2"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"t", " ", "=", " ", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"w", "*", "w"}], "+", "w"}], ")"}], "/", "2"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"y", " ", "=", " ", 
        RowBox[{"n", "-", "t"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"x", " ", "=", " ", 
        RowBox[{"w", "-", "y"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"Return", "[", 
        RowBox[{"{", 
         RowBox[{"x", ",", "y"}], "}"}], "]"}]}]}], "\[IndentingNewLine]", 
     "]"}]}]}]}]], "Input",
 CellChangeTimes->{{3.9173277986970444`*^9, 3.9173279559748306`*^9}, {
  3.9173280207625856`*^9, 3.917328120082919*^9}, {3.9173281897939014`*^9, 
