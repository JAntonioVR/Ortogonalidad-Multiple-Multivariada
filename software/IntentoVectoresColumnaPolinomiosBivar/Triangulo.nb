Cell[TextData[{
 "Definimos los productos escalares\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{"f", ",", "g"}], "\[RightAngleBracket]"}], "j"], "=", 
     RowBox[{
      SubsuperscriptBox["\[Integral]", "0", "1"], 
      RowBox[{
       SubsuperscriptBox["\[Integral]", "0", 
        RowBox[{"1", "-", "x"}]], 
       RowBox[{
        RowBox[{"f", "(", 
         RowBox[{"x", ",", "y"}], ")"}], 
        RowBox[{"g", "(", 
         RowBox[{"x", ",", "y"}], ")"}], 
        SuperscriptBox["x", 
         SubscriptBox["\[Alpha]", "j"]], 
        SuperscriptBox[
         RowBox[{
          SuperscriptBox["y", "\[Beta]"], "(", 
          RowBox[{"1", "-", "x", "-", "y"}], ")"}], "\[Gamma]"], 
        RowBox[{"\[DifferentialD]", "y"}], 
        RowBox[{"\[DifferentialD]", "x"}]}]}]}]}], ",", " ", 
    RowBox[{"j", "=", "1"}], ",", "2", ",", "3"}], TraditionalForm]],
  ExpressionUUID->"8b2c85d7-8693-4517-b100-281ceabcda7d"],
 ".\nY sus respectivos funcionales y productos escalares generalizados a \
vectores de polinomios.\n"
