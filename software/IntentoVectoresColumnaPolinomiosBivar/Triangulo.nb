          RowBox[{"(", 
           RowBox[{
            RowBox[{"terna", "[", 
             RowBox[{"[", "2", "]"}], "]"}], "+", "1"}], ")"}]}], "+", 
         RowBox[{
          RowBox[{"terna", "[", 
           RowBox[{"[", "3", "]"}], "]"}], 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"terna", "[", 
             RowBox[{"[", "3", "]"}], "]"}], "+", "1"}], ")"}]}]}], "]"}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"nin", " ", "=", " ", 
      RowBox[{"NIncognitas", "[", 
       RowBox[{"n", ",", "2"}], "]"}]}], ";", "\[IndentingNewLine]", 
