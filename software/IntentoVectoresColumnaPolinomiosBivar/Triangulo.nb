Cell[BoxData[
 RowBox[{
  RowBox[{"ConstruyePolinomio", "[", "terna_", "]"}], ":=", " ", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "n", ",", "nin", ",", "incognitas", ",", "G", ",", "X", ",", "Pn", ",", 
      "ecs", ",", "ecs1", ",", "ecs2", ",", "ecs3", ",", "ecuaciones", ",", 
      "sol"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
