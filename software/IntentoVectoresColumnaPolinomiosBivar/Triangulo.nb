     "\[IndentingNewLine]", 
     RowBox[{"ecs", " ", "=", " ", 
      RowBox[{"Join", "[", 
       RowBox[{"ecs1", ",", " ", "ecs2", ",", " ", "ecs3"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
