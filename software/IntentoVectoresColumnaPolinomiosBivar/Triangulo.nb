         RowBox[{"i", ",", "1", ",", "nin"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"G", " ", "=", " ", 
      RowBox[{"MatricesG", "[", "n", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"X", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"MonomiosGrado", "[", "i", "]"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", "0", ",", "n"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Pn", " ", "=", " ", 
      RowBox[{
       RowBox[{"X", "[", 
        RowBox[{"[", 
